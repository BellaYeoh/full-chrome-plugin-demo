{
  "manifest_version": 2,
  "name": "chrome-full-demo",
  "version": "1.0.0",
  "description": "常用的扩展程序实例",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "这是一个示例chrome插件",
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus", "notifications"]
}
